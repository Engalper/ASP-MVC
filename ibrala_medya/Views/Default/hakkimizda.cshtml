@model List<ibrala_medya.Models.hakkimizda>

@{
    ViewBag.Title = "Hakkımızda";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<!-- Breadcrumbs Start -->
<div class="rs-breadcrumbs">
    <div class="breadcrumbs-wrap">
        <img src="~/Content/images/breadcrumbs/" alt="Breadcrumbs Image">
        <div class="breadcrumbs-inner">
            <div class="container">
                <div class="breadcrumbs-text">
                    <h1 class="breadcrumbs-title"></h1>

                </div>
            </div>
        </div>
    </div>
</div>
<!-- Breadcrumbs End -->
@foreach (var hakkimizda in Model)
{


<!-- About Section Start-->
<div id="rs-about" class="rs-about pt-130 pb-130 md-pt-100 md-pb-100">
    <div class="container">
        <div class="row align-items-center pb-100 md-pb-50">
            <div class="col-lg-7 md-mb-30">
                <div class="about-img wow fadeInLeft" data-wow-delay="300ms" data-wow-duration="2000ms">
                    <img src="~/Content/images/about/@hakkimizda.icerikresim" alt="">
                </div>
            </div>
            <div class="col-lg-5">
                <div class="sec-title mb-30">
                    <h2 class="title wow fadeInLeft" data-wow-delay="300ms" data-wow-duration="2000ms">@hakkimizda.icerikbaslik</h2>
                    <p class="wow fadeInLeft" data-wow-delay="300ms" data-wow-duration="2000ms">@hakkimizda.icerikmetin</p>
                </div>
                <div class="about-checkbox">
                    <ul class="check-list-box wow fadeInLeft" data-wow-delay="300ms" data-wow-duration="2000ms">
                        <li>@hakkimizda.icerikmadde1</li>
                        <li>@hakkimizda.icerikmadde2</li>
                        <li>@hakkimizda.icerikmadde3</li>
                    </ul>
                </div>
                <!--<div class="about-btn-part mt-40 wow fadeInLeft" data-wow-delay="300ms" data-wow-duration="2000ms">
                    <a class="readon" href="#">DahaSonra</a>
                </div>-->
            </div>
        </div>
    </div><!-- .container end -->
</div>

<!-- About Section End-->

<!-- Testimonial Section Start -->
<div id="rs-testimonial" class="rs-testimonial pt-130 pb-130 md-pt-100 md-pb-100" style="margin-top:-200px;">
    <div class="sec-title text-center mb-60">
        <h2 class="title title-color">Müşterilerimizin Görüşleri</h2>
        <p class="desc global-color margin-0">Slogan bazlı içerik</p>
    </div>
    <div class="container">
        <div class="rs-carousel owl-carousel"
             data-loop="true"
             data-items="3"
             data-margin="30"
             data-nav="false"
             data-dots="false"
             data-autoplay="true"
             data-hoverpause="true"
             data-autoplay-timeout="5000"
             data-smart-speed="800"
             data-nav-speed="false"
             data-center="false"
             data-items-md="2"
             data-margin-md="20"
             data-center-md="false"
             data-nav-md="false"
             data-dots-md="false"
             data-items-sm="1"
             data-nav-sm="false"
             data-dots-sm="false"
             data-margin-sm="0"
             data-items-xs="1"
             data-nav-xs="false"
             data-dots-xs="false"
             data-margin-xs="0">
            <div class="single-testimonial box-shadow text-center">
                <div class="author-desc">
                    <span class="author-icon">
                        <i class="flaticon-left-quote"></i>
                    </span>
                    <p class="author-txt"></p>
                </div>
                <div class="testimonial-author">
                    <div class="author-img">
                        <img src="images/testimonial/" alt="">
                    </div>
                    <div class="author-info">
                        <h4 class="author-title"></h4>
                        <span class="author-post"></span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Testimonial Section End -->
<!-- Team  Section Start Here -->
<div class="rs-team">
    <div class="container">
        <div class="sec-title text-center mb-60">
            <h2 class="title title-color">Çalışanlarımız</h2>
            <p class="desc global-color margin-0">Slogan bazlı içerik</p>
        </div>
        <div class="rs-carousel owl-carousel"
             data-loop="true"
             data-items="3"
             data-margin="30"
             data-nav="false"
             data-dots="false"
             data-autoplay="true"
             data-hoverpause="true"
             data-autoplay-timeout="5000"
             data-smart-speed="800"
             data-nav-speed="false"
             data-center="true"
             data-items-md="2"
             data-margin-md="20"
             data-center-md="false"
             data-nav-md="false"
             data-dots-md="false"
             data-items-sm="1"
             data-nav-sm="false"
             data-dots-sm="false"
             data-margin-sm="10"
             data-items-xs="1"
             data-nav-xs="false"
             data-dots-xs="false"
             data-margin-xs="10">
            <div class="item">
                <div class="team-item">
                    <div class="img-part">
                        <img src="~/Content/images/team/" alt="">
                    </div>
                    <div class="content-part">
                        <h3 class="name">
                            @Html.ActionLink("Görünecek İsim", "calisanlar", new { id=hakkimizda.haid})
                            <a class="title" href="team-single.html"></a>
                        </h3>
                        <span class="category"></span>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
<!-- Team  Section Start Here -->
<!-- Partner Slider Start -->
<div id="rs-partner" class="rs-partner pt-130 pb-130 md-pt-100 md-pb-100">
    <div class="container">
        <div class="rs-carousel owl-carousel"
             data-loop="true"
             data-items="5"
             data-margin="30"
             data-nav="false"
             data-dots="false"
             data-autoplay="true"
             data-hoverpause="true"
             data-autoplay-timeout="5000"
             data-smart-speed="800"
             data-nav-speed="false"
             data-center="false"
             data-items-md="3"
             data-margin-md="30"
             data-center-md="false"
             data-nav-md="false"
             data-dots-md="false"
             data-items-sm="2"
             data-nav-sm="false"
             data-dots-sm="false"
             data-margin-sm="20"
             data-items-xs="1"
             data-nav-xs="false"
             data-dots-xs="false"
             data-margin-xs="0">
            <div class="partner-item">
                <a href="#"><img src="~/Content/images/partner/1.png" alt="Partner Image"></a>
            </div>
            <div class="partner-item">
                <a href="#"><img src="~/Content/images/partner/2.png" alt="Partner Image"></a>
            </div>
            <div class="partner-item">
                <a href="#"><img src="~/Content/images/partner/3.png" alt="Partner Image"></a>
            </div>
            <div class="partner-item">
                <a href="#"><img src="~/Content/images/partner/4.png" alt="Partner Image"></a>
            </div>
            <div class="partner-item">
                <a href="#"><img src="~/Content/images/partner/5.png" alt="Partner Image"></a>
            </div>
            <div class="partner-item">
                <a href="#"><img src="~/Content/images/partner/6.png" alt="Partner Image"></a>
            </div>
            <div class="partner-item">
                <a href="#"><img src="~/Content/images/partner/7.png" alt="Partner Image"></a>
            </div>
        </div> <!-- //.rs-carousel -->
    </div>
</div>
}
<!-- Partner Slider End -->
